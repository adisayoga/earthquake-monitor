    <!-- Dialog -->
    <string name="dialog_startup_title">Pemberitahuan</string>
    <string name="dialog_startup_msg">Aplikasi ini dapat berjalan secara periodik
		untuk menemukan kejadian gempa bumi dan mengirim pemberitahuan. Anda dapat 
		mengaktifkan pemberitahuan ini melalui Menu &gt; Pengaturan.\n\nKetika 
		pemberitahuan tidak aktif, anda tidak dapat menerima pemberitahuan otomatis, 
		dan anda harus me-refresh secara manual.\n\nAplikasi juga dapat memposting 
		informasi gempa ke wall Facebook, tetapi sebelumnya harus login dahulu 
		menggunakan akun Facebook anda.</string>
	<string name="dialog_network_error_title">Jaringan Error</string>
    <string name="dialog_network_error_msg">Tidak bisa mendapatkan data gempa bumi terbaru.\n\nPastikan anda terhubung ke internet (mobile atau wifi).</string>
    <string name="dialog_warn_interval_title">Peringatan</string>
    <string name="dialog_warn_interval_msg">Gunakan interval &quot;tak tepat&quot; (15 menit,
		setengah jam, satu jam, atau satu hari) untuk menghemat baterai. Ini berarti
		mendapatkan data mungkin tidak akan sesuai dengan yang anda kira.</string>
    	
